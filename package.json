{
  "name": "our-family-ts",
  "version": "1.0.0",
  "description": "photo album and family tree",
  "main": "main.js",
  "type": "module",
  "moduleResolution": "Node",
  "sideEffects": [
    "*.css"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "build:backend": "rimraf ./build && npx tsc",
    "start": "node build/main.js",
    "server": "nodemon",
    "start:server": "npm run build:backend && node build/main",
    "client": "npm run dev --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "lint": "npx eslint ./",
    "format": "npx eslint ./ --fix"
  },
  "keywords": [
    "family",
    "tree"
  ],
  "author": "Yuri Serikov <userikov@yandex.ru>",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^5.5.0",
    "mongoose": "^7.2.2",
    "morgan": "^1.10.0",
    "neo4j-driver": "^5.9.0",
    "nodemailer": "^6.9.3",
    "react-bootstrap": "^2.8.0",
    "uuid": "^9.0.0"
  }
}
